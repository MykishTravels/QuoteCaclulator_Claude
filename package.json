  "description": "Multi-Resort Travel Quote Engine - Calculation and pricing system for luxury travel consultants",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
